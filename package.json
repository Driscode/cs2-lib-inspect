{
    "name": "@ianlucas/cs2-lib-inspect",
    "version": "1.0.0",
    "description": "A utility for creating inspect links for CS2 items",
    "license": "MIT",
    "repository": "ianlucas/cs2-lib-inspect",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsup --legacy-output",
        "format": "prettier . --write",
        "genproto": "npx protoc --ts_out src --proto_path protos protos/econ.proto",
        "test": "jest"
    },
    "dependencies": {
        "@protobuf-ts/runtime": "^2.9.4",
        "buffer": "^6.0.3",
        "crc-32": "^1.2.2"
    },
    "peerDependencies": {
        "@ianlucas/cs2-lib": "^4.5.0"
    },
    "devDependencies": {
        "@protobuf-ts/plugin": "^2.9.3",
        "prettier": "^3.2.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}
